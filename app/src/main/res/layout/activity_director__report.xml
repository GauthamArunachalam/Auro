        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Syudent Report"
            android:id="@+id/studentReport"/>
        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Attendance Report"
            android:id="@+id/attendanceReport"/>

        attendanceReport
    </LinearLayout>


</RelativeLayout>